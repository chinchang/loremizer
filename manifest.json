{
	"description": "lorem ipsum-izes any web page",
	"manifest_version": 2,
	"name": "loremizer",
	"version": "1.1",

	"browser_action": {
		"default_icon": "icons/off.png",
		"default_popup": "popup.html",
		"browser_style": true
	},
	"icons": {
		"16": "icons/off.png",
		"48": "icons/off.png"
	},
	"web_accessible_resources": [
		"fonts/*"
	],

	"permissions": ["activeTab", "<all_urls>", "tabs"]
}
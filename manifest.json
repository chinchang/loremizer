{
	"description": "lorem ipsum-izes any web page",
	"manifest_version": 2,
	"name": "loremizer",
	"version": "1.0",

	"background": {
		"scripts": ["background.js"]
	},

	"browser_action": {
		"default_icon": "icons/off.png",
		"browser_style": true
	},
	"icons": {
		"16": "icons/off.png",
		"48": "icons/off.png"
	},

	"permissions": ["activeTab", "tabs"]
}
